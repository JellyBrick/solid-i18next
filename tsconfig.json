{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "declaration": true,
        "declarationDir": "./types",
        "esModuleInterop": true,
        "jsx": "preserve",
        "jsxImportSource": "solid-js",
        "module": "esnext",
        "moduleResolution": "node",
        "outDir": "./dist",
        "resolveJsonModule": true,
        "target": "esnext"
    },
    "include": ["src"]
}
